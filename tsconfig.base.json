{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@angular/*": ["./node_modules/@angular/*"],
      "@app/*": ["src/app/*"],
      "@dom/components/card-button": [
        "libs/src/lib/dom/components/card-button/card-button.component"
      ],
      "@dom/components/divider-header": [
        "libs/src/lib/dom/components/divider-header/divider-header.component"
      ],
      "@dom/components/flip-container": [
        "libs/src/lib/dom/components/flip-container/flip-container.component"
      ],
      "@dom/components/floating-button": [
        "libs/src/lib/dom/components/floating-button/floating-button.component"
      ],
      "@dom/components/form/helpers": [
        "libs/src/lib/dom/components/form/helpers.ts"
      ],
      "@dom/components/form/inputs/form-autocomplete": [
        "libs/src/lib/dom/components/form/inputs/form-autocomplete/form-autocomplete.component.ts"
      ],
      "@dom/components/form/inputs/form-input": [
        "libs/src/lib/dom/components/form/inputs/form-input/form-input.component.ts"
      ],
      "@dom/components/form/inputs/form-search-input": [
        "libs/src/lib/dom/components/form/inputs/form-search/form-search.component.ts"
      ],
      "@dom/components/form/models": [
        "libs/src/lib/dom/components/form/models/input.types.ts"
      ],
      "@dom/components/form/services": [
        "libs/src/lib/dom/components/form/services/form-error.service.ts"
      ],
      "@dom/components/form/types": [
        "libs/src/lib/dom/components/form/types.ts"
      ],
      "@dom/components/info-card": [
        "libs/src/lib/dom/components/info-card/info-card.component"
      ],
      "@dom/components/table": ["libs/src/lib/dom/components/table/index.ts"],
      "@dom/directives": ["libs/src/lib/dom/directives/index.ts"],
      "@dom/helpers": ["libs/src/lib/dom/helpers/index.ts"],
      "ng-filters-service/abstract-filters": [
        "libs-utils/src/lib/ng-filters-lib/abstract-filters.ts"
      ],
      "ng-filters-service/evaluate-filters": [
        "libs-utils/src/lib/ng-filters-lib/evaluate-filter.service.ts"
      ],
      "ng-filters-service/filters.types": [
        "libs-utils/src/lib/ng-filters-lib/filter.types.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
